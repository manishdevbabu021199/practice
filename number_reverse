// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int number = 13456000;
    int reversedNumber = 0;
    int extractedDigit = 0;
    while(number > 0){
        extractedDigit = number%10;
        number = number/10;
        reversedNumber = reversedNumber * 10 + extractedDigit;
    }
    cout << reversedNumber;

    return 0;
}
